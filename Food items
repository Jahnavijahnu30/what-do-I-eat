#include<stdio.h>
int main()
{
	printf("Pick any food item : \n1. Pizza \n2. Burger \n3. Sandwich \n4. Pastha \n5. French Fries\n");
	int choice;
	scanf("%d", &choice);
	switch(choice)
	{
		case 1: printf("Food item : Pizza \nPrice : 220 ");
		break;
		case 2: printf("Food item : Burger \nPrice : 170 ");
		break;
		case 3: printf("Food item : Sandwich \nPrice : 190 ");
		break;
    case 4: printf("Food item : Pastha \nPrice : 150 ");
    break;
    case 5: printf("Food item : French Fries \nPrice : 90 ");
    break;
	}
}
